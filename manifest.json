{
  "manifest_version": 2,
  "name": "Savage Rabbit",
  "description": "This extension allows you to find people available in calendar",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/js/background.js",
      "src/js/hot-reload.js"
    ]
  }
}